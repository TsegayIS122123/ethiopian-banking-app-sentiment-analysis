# .env file - Environment variables for Ethiopian Banking Apps Analysis

# App IDs (use if you want to override hardcoded values)
CBE_APP_ID=com.combanketh.mobilebanking
BOA_APP_ID=com.boa.boaMobileBanking  
DASHEN_APP_ID=com.cr2.amolelight

# Scraping Configuration
REVIEWS_PER_BANK=450
MAX_RETRIES=3
RETRY_DELAY=5
REQUEST_TIMEOUT=30
SCRAPER_LANG=en
SCRAPER_COUNTRY=et
SORT_METHOD=NEWEST

# Processing Configuration
MIN_REVIEW_LENGTH=3
MAX_REVIEW_LENGTH=1000
ALLOWED_MISSING_PCT=5.0
TARGET_REVIEWS_PER_BANK=400
ENGLISH_RATIO_THRESHOLD=0.3

DB_HOST=localhost
DB_NAME=bank_reviews
DB_USER=postgres
DB_PASSWORD=123456
DB_PORT=5432